{
  "dashboard": {
    "id": null,
    "title": "Mock S3 System Overview",
    "tags": ["mock-s3", "overview"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Service Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"mock-s3-services\"}",
            "legendFormat": "{{service}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "System CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "system_cpu_usage_percent",
            "legendFormat": "{{service}} CPU %"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "System Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "system_memory_usage_percent",
            "legendFormat": "{{service}} Memory %"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 6}
      },
      {
        "id": 4,
        "title": "Machine Online Status",
        "type": "stat",
        "targets": [
          {
            "expr": "system_machine_online_status",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 6}
      }
    ],
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "refresh": "10s",
    "schemaVersion": 30,
    "version": 1
  }
}