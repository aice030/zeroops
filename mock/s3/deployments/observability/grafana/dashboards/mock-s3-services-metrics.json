{
  "id": null,
  "title": "Mock S3 Services Resource Metrics",
  "tags": [
    "mock-s3",
    "services",
    "resources"
  ],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "query": "label_values(system_cpu_usage_percent_percent, service)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "instance",
        "type": "query",
        "query": "label_values(system_cpu_usage_percent_percent{service=~\"$service\"}, instance)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Services CPU Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "system_cpu_usage_percent_percent{service=~\"$service\",instance=~\"$instance\"}",
          "legendFormat": "{{service}} {{instance}} CPU",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisPlacement": "auto",
            "axisLabel": "CPU Usage (%)",
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Services Memory Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "system_memory_usage_percent_percent{service=~\"$service\",instance=~\"$instance\"}",
          "legendFormat": "{{service}} {{instance}} Memory",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisPlacement": "auto",
            "axisLabel": "Memory Usage (%)",
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Current CPU Usage by Service",
      "type": "stat",
      "targets": [
        {
          "expr": "system_cpu_usage_percent_percent{service=~\"$service\",instance=~\"$instance\"}",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "orange",
                "value": 75
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 9
      }
    },
    {
      "id": 4,
      "title": "Current Memory Usage by Service",
      "type": "stat",
      "targets": [
        {
          "expr": "system_memory_usage_percent_percent{service=~\"$service\",instance=~\"$instance\"}",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 60
              },
              {
                "color": "orange",
                "value": 80
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 9
      }
    },
    {
      "id": 6,
      "title": "Services Network QPS",
      "type": "timeseries",
      "targets": [
        {
          "expr": "system_network_qps_per_second{service=~\"$service\",instance=~\"$instance\"}",
          "legendFormat": "{{service}} {{instance}} QPS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisPlacement": "auto",
            "axisLabel": "Queries/sec",
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20
          },
          "unit": "ops",
          "min": 0
        }
      },
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 17
      }
    },
    {
      "id": 7,
      "title": "Services Online Status",
      "type": "stat",
      "targets": [
        {
          "expr": "system_machine_online_status{service=~\"$service\",instance=~\"$instance\"}",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "Offline",
                  "color": "red"
                },
                "1": {
                  "text": "Online",
                  "color": "green"
                }
              }
            }
          ]
        }
      },
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 26
      }
    },
    {
      "id": 8,
      "title": "Resource Usage Comparison",
      "type": "bargauge",
      "targets": [
        {
          "expr": "system_cpu_usage_percent_percent{service=~\"$service\"}",
          "legendFormat": "{{service}} CPU",
          "refId": "A"
        },
        {
          "expr": "system_memory_usage_percent_percent{service=~\"$service\"}",
          "legendFormat": "{{service}} Memory",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 60
              },
              {
                "color": "orange",
                "value": 80
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "basic",
        "showUnfilled": true
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 32
      }
    },
    {
      "id": 9,
      "title": "Services Resource Usage Table",
      "type": "table",
      "targets": [
        {
          "expr": "system_cpu_usage_percent_percent{service=~\"$service\",instance=~\"$instance\"}",
          "legendFormat": "",
          "refId": "A",
          "instant": true,
          "format": "table"
        },
        {
          "expr": "system_memory_usage_percent_percent{service=~\"$service\",instance=~\"$instance\"}",
          "legendFormat": "",
          "refId": "B",
          "instant": true,
          "format": "table"
        },
        {
          "expr": "system_network_qps_per_second{service=~\"$service\",instance=~\"$instance\"}",
          "legendFormat": "",
          "refId": "C",
          "instant": true,
          "format": "table"
        },
        {
          "expr": "system_machine_online_status{service=~\"$service\",instance=~\"$instance\"}",
          "legendFormat": "",
          "refId": "D",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "job": true,
              "node": true,
              "otel_scope_name": true,
              "__name__": true
            },
            "renameByName": {
              "service": "Service",
              "instance": "Instance",
              "Value #A": "CPU (%)",
              "Value #B": "Memory (%)",
              "Value #C": "Network QPS",
              "Value #D": "Status"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "center",
            "displayMode": "color-background"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "orange",
                "value": 75
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Service"
            },
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "basic"
              },
              {
                "id": "custom.width",
                "value": 200
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Status"
            },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "0": {
                        "text": "Offline",
                        "color": "red"
                      },
                      "1": {
                        "text": "Online",
                        "color": "green"
                      }
                    }
                  }
                ]
              },
              {
                "id": "custom.displayMode",
                "value": "basic"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 42
      }
    },
    {
      "id": 10,
      "title": "Top Resource Consumers",
      "type": "piechart",
      "targets": [
        {
          "expr": "topk(5, system_cpu_usage_percent_percent{service=~\"$service\",instance=~\"$instance\"})",
          "legendFormat": "{{service}} {{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "percent"
        }
      },
      "options": {
        "pieType": "pie",
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "visible",
          "placement": "right"
        },
        "displayLabels": [
          "name",
          "value"
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 50
      }
    },
    {
      "id": 11,
      "title": "Average Resource Usage by Service",
      "type": "heatmap",
      "targets": [
        {
          "expr": "avg_over_time(system_cpu_usage_percent_percent{service=~\"$service\",instance=~\"$instance\"}[5m])",
          "legendFormat": "{{service}} {{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "spectrum"
          },
          "unit": "percent"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 50
      }
    }
  ],
  "annotations": {
    "list": []
  },
  "schemaVersion": 36,
  "version": 1
}
